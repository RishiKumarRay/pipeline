pipeline{
    agent{
        label any
    }
    stages{
        stage("Checkout scm"){
            steps{
                echo "git clone....."
            }
        }
        stage("Parallel Build"){
            parallel{
                stage("Build on Linux"){
                    
                    steps{
                       echo "building"
                    }
                }
                stage("Build on WIndows"){
                    
                    steps{
                        echo "building"
                    }
                }
                stage("Test"){
                    
                    steps{
                        JUNIT 
                    }
                }
            }
        }
    }
}
