pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
    
        }
     stage('Build'){
    steps {
        echo 'building'
                   
    }
    stage("testing") {
    parallel {
        stage("unit tests") {
        steps {
        echo "testing..."
        
}
}
}
}
}
}
}
